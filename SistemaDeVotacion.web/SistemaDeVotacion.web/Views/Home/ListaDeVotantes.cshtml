@model List<VoterViewModel>

<h2>Lista de Votantes</h2>

<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Dirección del Votante</th>
            <th scope="col">Nombre del Candidato</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var voter in Model)
        {
            <tr>
                <td>@voter.AddressVoter</td>
                <td>@voter.CandidateName</td>
            </tr>
        }
    </tbody>
</table>

<div class="text-center" style="margin-top: 20px;">
    <a href="@Url.Action("Index", "Home")" class="btn btn-secondary">Volver al Inicio</a>
</div>